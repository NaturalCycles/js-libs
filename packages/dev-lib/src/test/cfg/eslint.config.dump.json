{
  "language": "@/js",
  "languageOptions": {
    "ecmaVersion": 2026,
    "parser": "vue-eslint-parser@10.2.0",
    "parserOptions": {
      "extraFileExtensions": {
        "0": ".vue",
        "1": ".html"
      },
      "projectService": true
    },
    "sourceType": "module"
  },
  "linterOptions": {
    "reportUnusedDisableDirectives": 2
  },
  "plugins": [
    "@",
    "@typescript-eslint:@typescript-eslint/eslint-plugin@8.50.1",
    "unicorn:eslint-plugin-unicorn@62.0.0",
    "vue:eslint-plugin-vue@10.6.2",
    "vitest:vitest@1.6.4",
    "import-x:eslint-plugin-import-x@4.16.1",
    "simple-import-sort:eslint-plugin-simple-import-sort@12.1.1"
  ],
  "processor": "vue/vue",
  "rules": {
    "@typescript-eslint/class-literal-property-style": [2],
    "@typescript-eslint/consistent-type-assertions": [2],
    "@typescript-eslint/explicit-member-accessibility": [
      2,
      {
        "accessibility": "no-public",
        "overrides": {
          "parameterProperties": "off"
        }
      }
    ],
    "@typescript-eslint/method-signature-style": [2],
    "@typescript-eslint/naming-convention": [
      2,
      {
        "format": ["camelCase"],
        "selector": "default"
      },
      {
        "format": ["camelCase", "PascalCase"],
        "selector": "import"
      },
      {
        "filter": {
          "match": false,
          "regex": "[- ]"
        },
        "format": ["camelCase"],
        "leadingUnderscore": "allowSingleOrDouble",
        "selector": ["function", "parameter", "property", "method", "memberLike"],
        "trailingUnderscore": "allowSingleOrDouble"
      },
      {
        "format": ["camelCase", "UPPER_CASE"],
        "leadingUnderscore": "allowSingleOrDouble",
        "selector": "variable",
        "trailingUnderscore": "allowSingleOrDouble"
      },
      {
        "format": ["camelCase", "UPPER_CASE", "PascalCase"],
        "leadingUnderscore": "allowSingleOrDouble",
        "selector": "classProperty",
        "trailingUnderscore": "allowSingleOrDouble"
      },
      {
        "format": ["camelCase", "UPPER_CASE", "PascalCase", "snake_case"],
        "leadingUnderscore": "allowSingleOrDouble",
        "selector": ["objectLiteralProperty", "objectLiteralMethod", "typeProperty", "enumMember"],
        "trailingUnderscore": "allowSingleOrDouble"
      },
      {
        "format": null,
        "modifiers": ["destructured"],
        "selector": ["variable", "parameter"]
      },
      {
        "format": ["PascalCase"],
        "selector": "typeLike"
      },
      {
        "format": ["PascalCase", "UPPER_CASE"],
        "selector": "typeParameter"
      },
      {
        "format": null,
        "modifiers": ["requiresQuotes"],
        "selector": [
          "classProperty",
          "objectLiteralProperty",
          "typeProperty",
          "classMethod",
          "objectLiteralMethod",
          "typeMethod",
          "accessor",
          "enumMember"
        ]
      }
    ],
    "@typescript-eslint/no-unnecessary-qualifier": [2],
    "@typescript-eslint/prefer-find": [2],
    "@typescript-eslint/prefer-string-starts-ends-with": [2],
    "@typescript-eslint/return-await": [2, "always"],
    "complexity": [
      2,
      {
        "max": 40
      }
    ],
    "id-denylist": [
      2,
      "any",
      "Number",
      "number",
      "String",
      "string",
      "Boolean",
      "boolean",
      "Undefined",
      "undefined"
    ],
    "id-match": [
      2,
      "^.+$",
      {
        "classFields": false,
        "ignoreDestructuring": false,
        "onlyDeclarations": false,
        "properties": false
      }
    ],
    "import-x/export": [2],
    "import-x/no-useless-path-segments": [2],
    "logical-assignment-operators": [
      2,
      "always",
      {
        "enforceForIfStatements": true
      }
    ],
    "no-octal": [2],
    "no-unreachable-loop": [
      2,
      {
        "ignore": []
      }
    ],
    "no-useless-assignment": [2],
    "object-shorthand": [2],
    "simple-import-sort/exports": [2],
    "simple-import-sort/imports": [
      2,
      {
        "groups": [["^\\u0000", "^node:", "^@?\\w", "^", "^\\."]]
      }
    ],
    "spaced-comment": [
      2,
      "always",
      {
        "markers": ["/"]
      }
    ],
    "unicorn/expiring-todo-comments": [
      1,
      {
        "allowWarningComments": true,
        "ignore": [],
        "ignoreDatesOnPullRequests": true,
        "terms": ["todo", "fixme", "xxx"]
      }
    ],
    "unicorn/prefer-import-meta-properties": [2],
    "unicorn/relative-url-style": [2, "never"],
    "vitest/consistent-test-filename": [2],
    "vitest/no-unneeded-async-expect-function": [2],
    "vitest/padding-around-after-all-blocks": [2],
    "vitest/padding-around-after-each-blocks": [2],
    "vitest/padding-around-before-all-blocks": [2],
    "vitest/padding-around-before-each-blocks": [2],
    "vitest/padding-around-describe-blocks": [2],
    "vitest/prefer-strict-boolean-matchers": [2],
    "vitest/prefer-vi-mocked": [2],
    "vitest/valid-expect-in-promise": [2],
    "vue/attribute-hyphenation": [1],
    "vue/attributes-order": [1],
    "vue/block-order": [
      2,
      {
        "order": ["script", "template", "style"]
      }
    ],
    "vue/comment-directive": [2],
    "vue/component-api-style": [2, ["script-setup", "composition", "composition-vue2"]],
    "vue/component-definition-name-casing": [1],
    "vue/component-name-in-template-casing": [
      2,
      "PascalCase",
      {
        "registeredComponentsOnly": true
      }
    ],
    "vue/component-options-name-casing": [2, "PascalCase"],
    "vue/custom-event-name-casing": [2, "camelCase"],
    "vue/define-macros-order": [2],
    "vue/eqeqeq": [2],
    "vue/first-attribute-linebreak": [1],
    "vue/html-comment-content-spacing": [2],
    "vue/jsx-uses-vars": [2],
    "vue/match-component-file-name": [
      2,
      {
        "extensions": ["vue", "jsx"],
        "shouldMatchCase": true
      }
    ],
    "vue/match-component-import-name": [2],
    "vue/multi-word-component-names": [2],
    "vue/no-arrow-functions-in-watch": [2],
    "vue/no-async-in-computed-properties": [2],
    "vue/no-boolean-default": [2],
    "vue/no-child-content": [2],
    "vue/no-computed-properties-in-data": [2],
    "vue/no-deprecated-data-object-declaration": [2],
    "vue/no-deprecated-delete-set": [2],
    "vue/no-deprecated-dollar-listeners-api": [2],
    "vue/no-deprecated-dollar-scopedslots-api": [2],
    "vue/no-deprecated-events-api": [2],
    "vue/no-deprecated-filter": [2],
    "vue/no-deprecated-functional-template": [2],
    "vue/no-deprecated-html-element-is": [2],
    "vue/no-deprecated-inline-template": [2],
    "vue/no-deprecated-model-definition": [2],
    "vue/no-deprecated-props-default-this": [2],
    "vue/no-deprecated-router-link-tag-prop": [2],
    "vue/no-deprecated-scope-attribute": [2],
    "vue/no-deprecated-slot-attribute": [2],
    "vue/no-deprecated-slot-scope-attribute": [2],
    "vue/no-deprecated-v-bind-sync": [2],
    "vue/no-deprecated-v-is": [2],
    "vue/no-deprecated-v-on-native-modifier": [2],
    "vue/no-deprecated-v-on-number-modifiers": [2],
    "vue/no-deprecated-vue-config-keycodes": [2],
    "vue/no-dupe-keys": [2],
    "vue/no-dupe-v-else-if": [2],
    "vue/no-duplicate-attributes": [2],
    "vue/no-expose-after-await": [2],
    "vue/no-lifecycle-after-await": [2],
    "vue/no-lone-template": [1],
    "vue/no-multiple-objects-in-class": [2],
    "vue/no-mutating-props": [2],
    "vue/no-parsing-error": [2],
    "vue/no-ref-as-operand": [2],
    "vue/no-ref-object-reactivity-loss": [2],
    "vue/no-reserved-component-names": [2],
    "vue/no-reserved-keys": [2],
    "vue/no-reserved-props": [2],
    "vue/no-root-v-if": [2],
    "vue/no-shared-component-data": [2],
    "vue/no-side-effects-in-computed-properties": [2],
    "vue/no-template-key": [2],
    "vue/no-template-shadow": [1],
    "vue/no-textarea-mustache": [2],
    "vue/no-this-in-before-route-enter": [2],
    "vue/no-unused-components": [2],
    "vue/no-unused-vars": [2],
    "vue/no-use-computed-property-like-method": [2],
    "vue/no-use-v-if-with-v-for": [2],
    "vue/no-useless-template-attributes": [2],
    "vue/no-v-for-template-key-on-child": [2],
    "vue/no-v-html": [1],
    "vue/no-v-text-v-html-on-component": [2],
    "vue/no-watch-after-await": [2],
    "vue/one-component-per-file": [1],
    "vue/order-in-components": [1],
    "vue/padding-line-between-blocks": [2],
    "vue/prefer-define-options": [2],
    "vue/prefer-separate-static-class": [2],
    "vue/prop-name-casing": [1],
    "vue/require-component-is": [2],
    "vue/require-default-prop": [1],
    "vue/require-direct-export": [2],
    "vue/require-emit-validator": [2],
    "vue/require-explicit-emits": [2],
    "vue/require-expose": [2],
    "vue/require-macro-variable-name": [2],
    "vue/require-prop-type-constructor": [2],
    "vue/require-prop-types": [2],
    "vue/require-render-return": [2],
    "vue/require-slots-as-functions": [2],
    "vue/require-toggle-inside-transition": [2],
    "vue/require-typed-object-prop": [2],
    "vue/require-v-for-key": [2],
    "vue/require-valid-default-prop": [2],
    "vue/return-in-computed-property": [2],
    "vue/return-in-emits-validator": [2],
    "vue/this-in-template": [1],
    "vue/use-v-on-exact": [2],
    "vue/v-bind-style": [1],
    "vue/v-for-delimiter-style": [2, "of"],
    "vue/v-on-event-hyphenation": [
      1,
      "always",
      {
        "autofix": true
      }
    ],
    "vue/v-on-style": [1],
    "vue/v-slot-style": [1],
    "vue/valid-attribute-name": [2],
    "vue/valid-define-options": [2],
    "vue/valid-next-tick": [2],
    "vue/valid-template-root": [2],
    "vue/valid-v-bind": [2],
    "vue/valid-v-cloak": [2],
    "vue/valid-v-else": [2],
    "vue/valid-v-else-if": [2],
    "vue/valid-v-for": [2],
    "vue/valid-v-html": [2],
    "vue/valid-v-if": [2],
    "vue/valid-v-is": [2],
    "vue/valid-v-memo": [2],
    "vue/valid-v-model": [2],
    "vue/valid-v-on": [2],
    "vue/valid-v-once": [2],
    "vue/valid-v-pre": [2],
    "vue/valid-v-show": [2],
    "vue/valid-v-slot": [2],
    "vue/valid-v-text": [2]
  },
  "settings": {
    "vitest": {
      "typecheck": true
    }
  }
}
