{
  "name": "@naturalcycles/dev-lib",
  "type": "module",
  "version": "18.4.2",
  "scripts": {
    "tsx-debug": "tsx scripts/testScript.ts",
    "dev-lib": "tsx ./src/bin/dev-lib.ts",
    "bt": "tsx ./src/bin/dev-lib.ts bt && tsx scripts/eslintPrintConfig.script.ts && tsx scripts/oxlintPrintConfig.script.ts",
    "check": "tsx ./src/bin/dev-lib.ts check && tsx scripts/eslintPrintConfig.script.ts && tsx scripts/oxlintPrintConfig.script.ts",
    "typecheck": "tsx ./src/bin/dev-lib.ts typecheck",
    "clean": "tsx ./src/bin/dev-lib.ts clean",
    "build": "tsx ./src/bin/dev-lib.ts build",
    "out": "tsx ./src/bin/out.ts",
    "test": "tsx ./src/bin/dev-lib.ts test",
    "test-leaks": "tsx ./src/bin/dev-lib.ts test-leaks",
    "test-integration": "tsx ./src/bin/dev-lib.ts test-integration",
    "test-manual": "tsx ./src/bin/dev-lib.ts test-manual",
    "lint": "tsx scripts/eslintPrintConfig.script.ts && tsx scripts/oxlintPrintConfig.script.ts && tsx ./src/bin/dev-lib.ts lint",
    "eslint": "tsx ./src/bin/dev-lib.ts eslint",
    "eslint-no-fix": "tsx ./src/bin/dev-lib.ts eslint-no-fix",
    "biome-no-fix": "tsx ./src/bin/dev-lib.ts biome-no-fix",
    "oxlint": "tsx ./src/bin/dev-lib.ts oxlint",
    "oxlint-no-fix": "tsx ./src/bin/dev-lib.ts oxlint-no-fix",
    "lint-staged-debug": "tsx ./src/bin/dev-lib.ts lint-staged"
  },
  "dependencies": {
    "@biomejs/biome": "^2",
    "@eslint/js": "^9",
    "prompts": "^2",
    "@naturalcycles/js-lib": "workspace:^15",
    "@naturalcycles/nodejs-lib": "workspace:^15",
    "@vitest/coverage-v8": "^4",
    "@vitest/eslint-plugin": "^1",
    "eslint": "^9",
    "eslint-plugin-import-x": "^4",
    "eslint-plugin-oxlint": "^1",
    "eslint-plugin-simple-import-sort": "^12",
    "eslint-plugin-unicorn": "^62",
    "eslint-plugin-vue": "^10",
    "globals": "^17",
    "lint-staged": "^16",
    "micromatch": "^4",
    "mitm": "^1",
    "oxlint": "^1",
    "oxlint-tsgolint": "*",
    "prettier": "^3",
    "typescript-eslint": "^8",
    "vue-eslint-parser": "^10"
  },
  "peerDependencies": {
    "husky": "^9",
    "stylelint": "^16",
    "stylelint-config-standard-scss": "^16",
    "typescript": "^5"
  },
  "peerDependenciesMeta": {
    "stylelint": {
      "optional": true
    },
    "stylelint-config-standard-scss": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/mitm": "^1",
    "@types/node": "^25",
    "@types/prompts": "^2"
  },
  "exports": {
    "./cfg/": "./cfg/",
    "./cfg/biome.jsonc": "./cfg/biome.jsonc",
    "./cfg/eslint.config.js": "./cfg/eslint.config.js",
    "./cfg/oxlint.config.json": "./cfg/oxlint.config.json",
    "./cfg/prettier.config.js": "./cfg/prettier.config.js",
    "./cfg/stylelint.config.js": "./cfg/stylelint.config.js",
    "./cfg/tsconfig.src.json": "./cfg/tsconfig.src.json",
    "./cfg/tsconfig.scripts.json": "./cfg/tsconfig.scripts.json",
    "./cfg/tsconfig.e2e.json": "./cfg/tsconfig.e2e.json",
    "./cfg/tsconfig.backend.json": "./cfg/tsconfig.backend.json",
    "./cfg/tsconfig.frontend.json": "./cfg/tsconfig.frontend.json",
    "./cfg/vitest.config.js": "./cfg/vitest.config.js",
    ".": "./dist/index.js",
    "./testing": "./dist/testing/index.js",
    "./testing/time": "./dist/testing/time.js",
    "./testing/testOffline": "./dist/testing/testOffline.js"
  },
  "files": [
    "dist",
    "scripts/tsconfig.json",
    "cfg/overwrite/.gitignore",
    "cfg"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "out": "dist/bin/out.js",
    "dev-lib": "dist/bin/dev-lib.js"
  },
  "engines": {
    "node": ">=24.10.0"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:NaturalCycles/js-libs.git",
    "directory": "packages/dev-lib"
  },
  "description": "Set of opinionated configuration files and tools for common project needs",
  "author": "Natural Cycles Team",
  "license": "MIT"
}
